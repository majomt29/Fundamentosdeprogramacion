#include "stdafx.h"
#include "math.h"
#include <iostream>

using namespace std;

void main ()
{
	float a,b,c,x1,x2;
	cout<<"ingrese un numero para a:";
	cin>>a;
	cout<<"ingrese un numero para b:";
	cin>>b;
	cout<<"ingrese un numero para c:";
	cin>>c;

	x1=(-b+sqrt(b*b-4*a*c))/2*a;
	x2=(-b-sqrt(b*b-4*a*c))/2*a;

	cout<<"el resultado de x1 es:"<<x1<<endl;
	cout<<"el resultado de x2 es:"<<x2<<endl;
	
}
